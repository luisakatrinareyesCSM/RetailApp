<Window x:Class="lkPangilinan.RetailApp.Windows.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:lkPangilinan.RetailApp.Windows"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="btnCustomers" Content="Customers" HorizontalAlignment="Left" Height="40" Margin="51,60,0,0" VerticalAlignment="Top" Width="194" Click="btnCustomers_Click"/>
        <Button x:Name="btnProducts" Content="Products" HorizontalAlignment="Left" Height="40" Margin="51,120,0,0" VerticalAlignment="Top" Width="194" Click="btnProducts_Click"/>
        <Button x:Name="btnUsers" Content="Users" HorizontalAlignment="Left" Height="40" Margin="51,180,0,0" VerticalAlignment="Top" Width="194" Click="btnUsers_Click"/>
        <Button x:Name="btnOrderProducts" Content="OrderProducts" HorizontalAlignment="Left" Height="40" Margin="51,240,0,0" VerticalAlignment="Top" Width="194" Click="btnOrderProducts_Click"/>
        <Button x:Name="btnOrders" Content="Users" HorizontalAlignment="Left" Height="40" Margin="51,300,0,0" VerticalAlignment="Top" Width="194" Click="btnOrders_Click"/>
    </Grid>
</Window>
